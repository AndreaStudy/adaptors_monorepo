{
  "name": "adaptors_fe_monorepo",
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "format": "prettier --cache --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "turbo": "^2.1.3"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "next-auth": "^4.24.10",
    "tailwind-scrollbar-hide": "^1.1.7",
    "zod": "^3.23.8"
  }
}
